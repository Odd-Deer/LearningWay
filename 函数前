year=int(input('year'))
month=int(input('month'))
if month==1 or month==3 or month==5 or month==7 or month==8 or month==10 or month==12:
    print(31)
elif month==4 or month==6 or month==9 or month==11 :
    print(30)
elif month==2 :
    if year % 4 == 0 and not year % 100 == 0 or year % 400 == 0:
     print(29)
    else:
     print(28)
else:
    print(404)

num1=float(input('num1'))
num2=float(input('num2'))
num3=float(input('num3'))
if num1>=num2 :
    if num1>=num3:
        print(rf'max={num1}')
        if num2>=num3 :
            print(rf'min={num3}')
        else :
            print(rf'min={num2}')
    else :
        print(rf"max={num3}"'\n'rf'min={num2}')
if num3>=num2 :
    if num3>=num1:
        print(rf'max={num3}')
        if num2>=num1 :
            print(rf'min={num1}')
        else :
            print(rf'min={num2}')
    else :
        print(rf"max={num1}"'\n'rf'min={num2}')
if num2>=num1 :
    if num2>=num3:
        print(rf'max={num2}')
        if num1>=num3 :
            print(rf'min={num3}')
        else :
            print(rf'min={num1}')
    else :
        print(rf"max={num3}"'\n'rf'min={num1}')
# 三个并非互斥，有交集的话就会出现打两次
num1 = float(input('num1: '))
num2 = float(input('num2: '))
num3 = float(input('num3: '))

# 先找最大值
if num1 >= num2 and num1 >= num3:
    max_val = num1
    # 找最小值（在 num1 是最大值的前提下）
    if num2 <= num3:
        min_val = num2
    else:
        min_val = num3
elif num2 >= num1 and num2 >= num3:
    max_val = num2
    # 找最小值（在 num2 是最大值的前提下）
    if num1 <= num3:
        min_val = num1
    else:
        min_val = num3
else:
    max_val = num3
    # 找最小值（在 num3 是最大值的前提下）
    if num1 <= num2:
        min_val = num1
    else:
        min_val = num2


temp=input('选择可乐，矿泉水或果汁')
all=int(input('给多少钱'))
if temp=='可乐':
    if all>3 :
        print(f'出货并找零{all-3}元')
    elif all==3 :
        print('出货不找零')
    else :
        print(f'还差{3-all}元')
if temp=='矿泉水':
    if all>2 :
        print(f'出货并找零{all-2}元')
    elif all==2 :
        print('出货不找零')
    else :
        print(f'还差{2-all}元')
if temp=='果汁':
    if all>5 :
        print(f'出货并找零{all-5}元')
    elif all==5 :
        print('出货不找零')
    else :
        print(f'还差{5-all}元')
i2=0
i3=0
i4=0
for i in range(1,101):
    if i%2==0:
        i2+=1
    if i%3==0:
        i3+=1
    if i%4==0:
        i4+=1
print(i2,i3,i4)


