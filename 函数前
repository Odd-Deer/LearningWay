numbers={9,1,3,5,6}
print("max is",max(numbers),'\n'"min is",min(numbers),'\n','average is',sum(numbers)/len(numbers))

str1="zxczxczxc"
length=len(str1)
word=input('输入查找的字符')
start=0
count=0
while True :
    if str1[start]==word:
        print(start)
        count+=1
    start+=1
    if start==length :
        if count==0 :
            print('没有这个字符')
        break
lis =[1,2,3,2,1,4,4,4,2,5,6,7,7,8,8,10,10]
new_list=set(lis)
print(new_list)
new_list=list(new_list)
print(new_list)


students =[
{'name':'小明','age':18,'score':98,'tel':'18888888888','gender':'female'},
{'name':'小红','age':20,'score':95,'tel':'18888888889','gender':'unkown'},
{'name':'小邓','age':18,'score':88,'tel':'18888888810','gender':'male'},
{'name':'小周','age':16,'score':58,'tel':'18888888811','gender':'unkown'},
{'name':'小林','age':19,'score':78,'tel':'18888888812','gender': 'male'},
{'name':'小薛','age':17,'score':92,'tel':'18888888813','gender': 'male'},
]
count=0
for i in students:
    if i['age']<18 :
        print(i)
    if i['score']>=90 :
        print(i['name'],i['score'])
    if i['gender']=='female':
        count+=1
print(count)

import random
suiji=random.randint(1,100)
while True :
    a=int(input('输入一个数字'))
    if a==suiji:
        print('right')
        break
    elif a>suiji:
        print('输的大了')
    else :
        print('输入得小了')


















