# 5、
apple=float(input("请输入购买苹果（个）"))
banana=float(input("请输入购买香蕉（kg）"))
orange=float(input("请输入购买橙子（组）"))
all=apple*2+banana*5+orange*4
print('一共',all,'元')
print(f'购买苹果{apple}个',
      f'购买香蕉{banana}kg',
      f'购买橙子{orange}组',sep='\n')
year=int(input('输入一个年份'))
if year%4==0 and not year%100==0 or year%400==0:
    print('闰年')
else:
    print('平年')

